<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="i">

	<!-- 대분류 모임 목록 조회 -->
	<select id = "big_list"
			resultType="category"
			parameterType="int"
			>
			
			select * from category
			where category_num like #{category}||'%'
	</select>


</mapper>























